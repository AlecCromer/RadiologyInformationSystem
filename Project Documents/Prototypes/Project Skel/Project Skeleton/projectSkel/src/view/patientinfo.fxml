<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.VBox?>

<VBox stylesheets="@../style.css" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.172-ea" fx:controller="Controller.PatientInfo">
    <ToolBar fx:id="barSearch">
        <items>
            <Button id="search" fx:id="buttonCloseSearch" stylesheets="@../style.css" text="search" />
            <TextField fx:id="fieldSearch" />
            <Button fx:id="buttonSearchUp" text="up" />
            <Button fx:id="buttonSearchDown" text="down" />
            <Label fx:id="labelMatches" />

        </items>
    </ToolBar>
    <Label fx:id="lblText" text="Hello" />
    <!--Button fx:id="btnBarTab" onAction="#handleButton" text="Change text"/-->

</VBox>
