<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="499.0" prefWidth="620.0" stylesheets="@../allViews.css" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.TechControllers.AddImageController">
   <children>
      <Button fx:id="select" layoutX="124.0" layoutY="26.0" mnemonicParsing="false" onAction="#captureImage" text="Select Image" AnchorPane.leftAnchor="124.0" AnchorPane.topAnchor="26.0" />
      <ComboBox fx:id="machineBox" layoutX="306.0" layoutY="26.0" prefHeight="25.0" prefWidth="191.0" promptText="Machine ID" AnchorPane.rightAnchor="123.0" AnchorPane.topAnchor="26.0" />
      <ImageView fx:id="imageview" fitHeight="373.0" fitWidth="373.0" layoutX="124.0" layoutY="63.0" pickOnBounds="true" preserveRatio="true" AnchorPane.topAnchor="63.0" />
      <Button fx:id="uploadImage" layoutX="265.0" layoutY="451.0" mnemonicParsing="false" onAction="#submitNewPhoto" text="Upload Image" AnchorPane.topAnchor="451.0" />
   </children>
</AnchorPane>
